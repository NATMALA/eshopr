<!DOCTYPE html>
<html>
<head>
    <title>Objednávky Primigistore.cz</title>
    <link rel="stylesheet" href="/public/default.css">
</head>
<body>
    <h2>Zboží</h2>
    <table>
        <tr>
            <th>Photo</th>
            <th>Objednávka</th>
            <th>Typ</th>
            <th>Artikl</th>
            <th>Velikost</th>
            <th>Cena</th>
            <th>Ks</th>
        </tr>
        <% productList.forEach((item, index) => { %>
            <tr>
                <td><img src="https://primatrend.cz/pmg/pic/<%= item.productId %>-1.jpg" alt="" border=1 height=75 width=75></img></td>
                <td><%= item.orderNumber %></td>
                <td><%= item.productType %></td>
                <td><%= item.productId %></td>
                <td><%= item.size %></td>
                <td><%= item.price %></td>
                <td><%= item.count %></td>
            </tr>
        <% }) %>
    </table>
    <p><a href="/">Zpátky</a></p>
</body>
</html>